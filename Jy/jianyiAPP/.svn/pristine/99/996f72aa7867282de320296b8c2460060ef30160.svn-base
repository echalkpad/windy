	<script src="../../js/route.js"></script>
	<script type="text/javascript">
		mui('.mui-content').on('tap', '.geneticTesting', function() {
			var type = this.className;
			mui.openWindow({
				id: 'geneticTestingDetail',
				url: "geneticTestingDetail.html",
				extras: {
					type: type,
				}
