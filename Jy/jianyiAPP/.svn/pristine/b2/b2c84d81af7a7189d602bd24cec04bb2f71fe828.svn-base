	<script type="text/javascript">
		mui.plusReady(function() {
			var self = plus.webview.currentWebview();
			var type = self.type;
			document.getElementById("spa-con1").style.display = 'none';
			document.getElementById("spa-con2").style.display = 'none';
			document.getElementById("spa-con3").style.display = 'none';
			document.getElementById("spa-con4").style.display = 'none';
			if (type == 'geneticTesting geneticTesting1') {
				document.getElementById("spa_tile").innerHTML = '肿瘤全套基因检测';
				document.getElementById("spa-con1").style.display = 'block';
			} else if (type == 'geneticTesting geneticTesting2') {
				document.getElementById("spa_tile").innerHTML = '心脑血管基因检测';
				document.getElementById("spa-con2").style.display = 'block';
			} else if (type == 'geneticTesting geneticTesting3') {
				document.getElementById("spa_tile").innerHTML = '烟酒茶（咖啡）损伤及成瘾套餐';
				document.getElementById("spa-con3").style.display = 'block';
			} else {
				document.getElementById("spa_tile").innerHTML = '健康尊享套餐（150项）';
				document.getElementById("spa-con4").style.display = 'block';
			}
		})
	</script>

