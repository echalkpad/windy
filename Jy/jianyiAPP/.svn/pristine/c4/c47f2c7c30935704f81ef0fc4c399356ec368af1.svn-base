		<style type="text/css">
			.mui-table-view{
				font-size:14px;
				color:#4A4A4A;
			}
			.mui-content>.mui-table-view:first-child{
				margin:0;
			}
			.mui-table-view:before,.mui-table-view-cell:after{
				background:#F1F1F1;
			}
			.mui-table-view-cell h2{
				font-size:16px;
				line-height:30px;
			}
			.mui-table-view-cell section{
				position: relative;
			}
			.mui-table-view-cell em{
				font-style: normal;
				position: absolute;
				right:0;
				top:50%;
				font-size:12px;
				color:#666;
				-webkit-transform:translateY(-50%);
			}
			.mui-table-view-cell img{
				width:50px;
				height:50px;
				border-radius:50%;
				background:#000;
				float:left;
				margin-right:10px;
			}
			.mui-table-view-cell section p{
				overflow:hidden;
				height:24px;
			}
			.xing-active {
				background-image: url(../../../img/pay/icon_star_fills.png)!important;
			}
			
			.xing {
				background: url(../../../img/pay/icon_star.png) no-repeat;
				background-size: 16px 16px;
				width: 16px;
				height: 16px;
				display:block;
				float:left;
				margin-right:4px;
			}
			p{line-height:20px;}
			.order_none{
				width:100px;
				position: fixed;
				top:30%;
				left:50%;
				margin-left:-50px;
				text-align: center;
				font-size:14px;
				color:#999;
			}
			.order_none i{
				display:block;
				width:30px;
				height:36px;
				margin:10px auto;
				background: url(../../../img/icon_dingdan.png) no-repeat scroll top left;
				background-size:cover;
			}
		</style>
