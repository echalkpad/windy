		<style type="text/css">
			#slowExpertsListShowCityPicker {
				line-height: 21px;
				width: 100%;
				height: 40px;
				margin-bottom: 15px;
				padding: 10px 8px;
				-webkit-user-select: text;
				border: 1px solid rgba(0, 0, 0, .2);
				border-radius: 3px;
				outline: 0;
				background-color: #fff;
				-webkit-appearance: none;
				font-size: 14px;
				height: auto;
				margin-top: 1px;
				border: 0!important;
				background-color: #fff;
				text-overflow: ellipsis;
				overflow:hidden;
				white-space: nowrap;
				text-align:center;
			}
			
			.mui-content {
				background: none;
			}
			.hospitalList{
				position:absolute;
				top:100px;
				width:100%;
			}
			.hospitalList li:after {
				display: block;
				content: "";
				clear: both;
			}
			
			.hospitalList li img {
				width: 70px;
				height: 70px;
				float: left;
				display: block;
				margin-right: 10px;
			}
			
			.hospitalList h3 {
				font-size: 18px;
				line-height: 30px;
				overflow: hidden;
				text-overflow: ellipsis;
				white-space: nowrap;
			}
			
			.hospitalList p {
				font-size: 16px;
				line-height: 24px;
				overflow: hidden;
				text-overflow: ellipsis;
				white-space: nowrap;
			}
			
			.demand {
				background: #fff;
				height: 50px;
				list-style: none;
				padding: 0;
				overflow: hidden;
				margin: 0;
				position: fixed;
			    top: 50px;
			    z-index: 999;
			    width: 100%;
			    border-bottom: 2px solid #F1F1F1;
			}
			
			.demand li:nth-child(1) {
				width: 25%;
				float: left;
				position: relative;
				padding: 5px 0 0;
			}
			
			.demand li:nth-child(1) label {
				font-family: Muiicons;
				color: #bbb;
				/*position: absolute;
				right: 0;
				top: 15px;*/
			}
			
			.demand select {
				background: none;
				margin: 0;
			}
			.sea {
				position: absolute;
				top: 16px;
				left: 25px;
				display: inline-block;
				width: 18px;
				height: 19px;
				background: url(../../img/icon_search.png) no-repeat scroll center center;
				background-size: cover;
			}
			.demand li:nth-child(2) {
				width: 75%;
				float: left;
				position: relative;
				padding: 10px 15px;
				background: #fff;
			}
			
			.demand li:nth-child(2) input {
				background: #f1f1f1;
				border-radius: 16px;
				border: 0;
				margin: 0;
				height: 32px;
				line-height: 32px;
				padding-left: 35px;
				color: #444;
			}
			.search {
			background: #f2f6f9!important;
			height: 300px;
			position: fixed;
			width:100%;
			top:100px;
			z-index:999;
		}
		
		.search .mui-popover {
			top: 0;
			height: 300px;
			background: #fff;
		}
		
		.search .mui-popover .mui-table-view {
			background: #fff;
		}
		
		.mui-table-view-cell{padding:11.5px 15px;}
		.search ul li a {
			color: #4A4A4A;
			padding:0;
			margin:0;
		}
		
		.mui-backdrop {
			top:100px;
		}
		
		#cancerExpertsDetails {
			position: relative;
			top: -260px;
		}
		
		
		#search-qg .search-qg-l {
			left: initial;
			width: 35%;
		}
		#search-qg .search-qg-r {
		    background: rgba(000, 000, 000, .2);
		    right: 0;
		    left: initial;
		    width: 65%;
		    border-radius: 0;
		    margin: 0;
		}
		#search-qg .search-qg-l ul,
		#search-qg .search-qg-r ul {
			padding-left: 0;
			list-style: none;
		}
		.search-ul li{
		    line-height: 44px;
		    height: 44px;
		    padding-left: 15px;
		}
		#householdDevices,#search-qg{
			height:100%;
		}
		.mui-popover .mui-scroll-wrapper{
			margin:0;
			border-radius: 0;
		}
		.mui-popover.mui-popover-action .mui-table-view{
			margin:0;
			border-radius: 0;
			background:#fff; 
			color:#333;
			text-align:left;
		}
		.mui-table-view-cell>a:not(.mui-btn) {
			padding-right: 25px;
		}
		
		.mui-navigate-right:after,
		.mui-push-right:after {
			right: 5px;
		}
		.zanwu_none{
			display: none;
			width:100px;
			position: fixed;
			top:30%;
			left:50%;
			margin-left:-50px;
			text-align: center;
			font-size:14px;
			color:#999;
		}
		.zanwu_none i{
			display:block;
			width:30px;
			height:36px;
			margin:10px auto;
			background: url(../../img/icon_dingdan.png) no-repeat scroll top left;
			background-size:cover;
		}
	</style>
