		<style type="text/css">
			html,
			body {
				background: rgb(242, 246, 249)!important;
			}
			
			.mui-table-view-cell:after,
			.mui-table-view:after {
				height: 0;
			}
			
			.mui-segmented-control {
				background-color: #fff;
				border: none;
			}
			
			.mui-segmented-control .mui-control-item.mui-active {
				position: relative;
				background-color: #fff;
				color: #00B6F3;

			}
			
			.mui-segmented-control .mui-control-item.mui-active:after {
				position: absolute;
				content: '';
				width: 100px;
				bottom: 0;
				left: 50%;
				margin-left: -50px;
				border-bottom: 1px solid #00B6F3;
			}
			
			.mui-segmented-control .mui-control-item {
				background-color: #fff;
				color: #000;
				border: none
			}
			
			.mui-table-view .mui-media-object.mui-pull-left {
				width: 60px;
				height: 60px;
				line-height: 60px;
				border-radius: 50%;
				max-width: 60px;
			}
			
			.mui-table-view-cell h2 {
				letter-spacing: -1px;
				overflow: hidden;
			}
			
			.mui-table-view-cell h2 span:nth-child(1) {
				float: left;
				height: 20px;
				line-height: 20px;
				font-size: 16px;
			}
			
			.mui-ios .mui-table-view-cell {
				border-bottom: 1px solid rgb(242, 246, 249);
			}
			
			.mui-table-view-cell h2 span:nth-child(2) {
				float: left;
				margin-left: 10px;
				height: 20px;
				line-height: 20px;
				font-size: 13px;

			}
			
			.mui-table-view-cell h2 span:nth-child(3) {
				height: 20px;
				line-height: 20px;
				font-size: 13px;
				color: #8f8f94;
				float: right;
				margin-right: 10px;
			}
			
			.mui-media-body p {
				height: 40px;
				line-height: 40px;
			}
			
			.mui-media-body p .pay {
				text-align: center;
				margin-top: 5px;
				width: 70px;
				height: 30px;
				line-height: 30px;
				display: block;
				float: right;
				margin-right: 10px;
				border:1px solid #f00;
				color: #f00;
				border-radius: 5px;
			}
			
			.mui-media-body p .pay-end {
				display: block;
				float: right;
				margin-right: 10px;
				color: rgb(240, 145, 53);
			}
			.mui-media{
				position: relative;
			}
			.mui-media-body p .pay-end img{
				position: absolute;
				bottom: 0;
				right: 20px;
				width: 70px;
			}
			.order_none{
				width:100px;
				position: fixed;
				top:30%;
				left:50%;
				margin-left:-50px;
				text-align: center;
				font-size:14px;
				color:#999;
			}
			.order_none i{
				display:block;
				width:30px;
				height:36px;
				margin:10px auto;
				background: url(../../img/icon_dingdan.png) no-repeat scroll top left;
				background-size:cover;
			}
		</style>
