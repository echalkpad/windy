		<style type="text/css">
			.mui-content {
				background: none;
			}
			
			.mui-table-view:before {
				display: none;
			}
			
			.mui-table-view-cell>a:not(.mui-btn) {
				padding-right: 70px;
				white-space: normal;
			}
			
			.mui-table-view-cell i {
				display: block;
				width: 26px;
				height: 26px;
				position: absolute;
				right: 15px;
				top: 50%;
				margin-top: -13px;
			}
			
			.mui-table-view-cell i {
				background: url(../../img/me/btn_info.png) no-repeat scroll center center;
				background-size: cover;
			}
			
			.mui-table-view p {
				font-size: 12px;
			}
			
			.mui-table-view h3 {
				color: #333;
				font-size: 16px;
				line-height: 24px;
			}
		</style>
