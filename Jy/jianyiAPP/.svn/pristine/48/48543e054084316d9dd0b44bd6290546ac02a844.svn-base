			html,
			body {
				background: rgb(242, 246, 249)!important;
			}
			
			.mui-content {
				background: none;
			}
			
			.mui-table-view-cell:after,
			.mui-table-view:after {
				height: 0;
			}
			
			.mui-segmented-control {
				background-color: #fff;
				border: none;
			}
			
			.mui-segmented-control .mui-control-item.mui-active {
				position: relative;
				background-color: #fff;
				color: #00B6F3;
			}
			
			.mui-segmented-control .mui-control-item.mui-active:after {
				position: absolute;
				content: '';
				width: 100px;
				bottom: 0;
				left: 50%;
				margin-left: -50px;
				border-bottom: 1px solid #00B6F3;
			}
			
			.mui-segmented-control .mui-control-item {
				background-color: #fff;
				color: #000;
				border: none
			}
			
			.mui-table-view .mui-media-object.mui-pull-left {
				width: 60px;
				height: 60px;
				line-height: 60px;
				border-radius: 50%;
				max-width: 60px;
			}
			
			.mui-table-view-cell h2 {
				letter-spacing: -1px;
				overflow: hidden;
			}
			
			.mui-table-view-cell h2 span {
				line-height: 34px;
				float: left;
			}
			
			.mui-table-view-cell h2 span:nth-child(1) {
				float: left;
				font-size: 16px;
			}
			
			.mui-ios .mui-table-view-cell {
				border-bottom: 1px solid rgb(242, 246, 249);
			}
			
			.mui-table-view-cell h2 span:nth-child(2) {
				margin-left: 10px;
				font-size: 14px;
			}
			
			.mui-table-view-cell h2 span:nth-child(2) {
				font-size: 13px;
				color: #8f8f94;
				float: right;
				margin-right: 10px;
			}
			
			.mui-media-body p {
				line-height: 20px;
				font-size: 13px;
			}
			
			.mui-media .pay {
				text-align: center;
				padding: 0 15px;
				line-height: 20px;
				display: block;
				position: absolute;
				right: 0;
				top: 6px;
				border: 1px solid #f00;
				color: #f00;
				border-radius: 3px;
			}
			.xing-active {
				background-image: url(../../../img/pay/icon_star_fills.png)!important;
			}
			
			.xing {
				background: url(../../../img/pay/icon_star.png) no-repeat;
				background-size: 16px 16px;
				width: 16px;
				height: 16px;
				display:block;
				float:left;
				margin-right:4px;
			}
			.pay-end {
				display: block;
				float: right;
				margin-right: 10px;
				color: rgb(240, 145, 53);
			}
			
			.mui-media {
				position: relative;
			}
			
			.pay-end img {
				position: absolute;
				bottom: -10px;
				right: 0;
				width: 70px;
			}
			
			.mui-table-view {
				background: none;
			}
			
			.mui-table-view-cell {
				margin-bottom: 10px;
				background: #fff;
			}
			
