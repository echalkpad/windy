			.circle {
				width: 64px;
				height: 64px;
				border-radius: 50%;
				background: #FE9808;
				position: absolute;
				top: 30px;
				left: 50%;
				-webkit-transform: translate(-50%, 0);
				display: none;
			}
			
			.circle_bottom {
				width: 100%;
				height: 20px;
				position: absolute;
				bottom: -8px;
				background: #fff;
			}
			
			.circle .pie_left,
			.circle .pie_right {
				width: 64px;
				height: 64px;
				position: absolute;
				top: 0;
				left: 0;
			}
			
			.circle .left,
			.circle .right {
				display: block;
				width: 64px;
				height: 64px;
				background: #E9E9E8;
				border-radius: 50%;
				position: absolute;
				top: 0;
				left: 0;
				transition: all 1s;
				-webkit-transition: all 1s;
			}
			
			.circle .pie_right,
			.circle .right {
				clip: rect(0, auto, auto, 32px);
			}
			
			.circle .pie_left,
			.circle .left {
				clip: rect(0, 32px, auto, 0);
			}
			
			.circle .mask {
				width: 56px;
				height: 56px;
				border-radius: 50%;
				left: 4px;
				top: 4px;
				background: #fff;
				position: absolute;
				text-align: center;
				font-size: 18px;
				padding-top: 5px;
				box-sizing: border-box;
				padding-top: 8px;
				transition: all 1s;
				-webkit-transition: all 1s;
			}
			
			.circle .mask h3 {
				color: #FE9808;
				line-height: 16px;
				font-size: 16px;
			}
			
			.circle .mask em {
