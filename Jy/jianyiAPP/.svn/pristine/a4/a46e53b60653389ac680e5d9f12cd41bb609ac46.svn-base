			}
			
			.mui-backdrop {
				top: 94px;
			}
			.search-qg .search-qg-l {
				left: initial;
				width: 100%;
				background:#fff;
			}
			
			.search-qg .search-qg-r {
				background: rgba(000, 000, 000, .2);
				right: 0;
				left: initial;
				width: 65%;
				border-radius: 0;
				margin: 0;
			}
			
			.search-qg .search-qg-l ul,
			.search-qg .search-qg-r ul {
				padding-left: 0;
				list-style: none;
			}
			
			.search-qg .search-qg-l ul li,
